{
  "name": "effector-factory-example",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14 <=18"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "effector": "^22.3.0",
    "effector-factorio": "^1.0.0",
    "effector-react": "^22.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.46",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@craco/craco": "^7.0.0-alpha.7",
    "eslint-plugin-effector": "^0.8.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src && tsc -b",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:effector/recommended",
      "plugin:effector/scope",
      "plugin:effector/react",
      "plugin:effector/future"
    ],
    "plugins": [
